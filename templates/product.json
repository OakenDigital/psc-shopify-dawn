        "custom_liquid_mdwrBa": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product-sku-display\" style=\"text-align: left; margin: 1rem 0;\">\n  <p style=\"margin: 0;\">\n    <strong>Part Number:</strong> \n    <span id=\"variant-sku-display\">\n      {{ product.selected_or_first_available_variant.sku | default: product.variants.first.sku }}\n    </span>\n  </p>\n</div>\n\n<script>\n(function() {\n  const display = document.getElementById('variant-sku-display');\n  const variantData = {{ product.variants | json }};\n  // Find the product form\n  const form = document.querySelector('form[action*=\"/cart/add\"]');\n\n  if (!display || !form || !variantData) return;\n\n  const updateSKU = () => {\n    // 1. Get the current Variant ID from the form's hidden input\n    const idInput = form.querySelector('[name=\"id\"]');\n    const currentId = idInput ? idInput.value : null;\n\n    // 2. Find the variant in our data list\n    const matchedVariant = variantData.find(v => v.id == currentId);\n\n    // 3. Logic for display\n    if (matchedVariant) {\n      // Combo exists! Show SKU (even if out of stock)\n      display.textContent = matchedVariant.sku || \"No SKU Assigned\";\n      display.style.opacity = \"1\";\n    } else {\n      // Combo does NOT exist (Unavailable)\n      display.textContent = \"Not Available\";\n      display.style.opacity = \"0.5\";\n    }\n  };\n\n  // Listen for changes anywhere in the form\n  form.addEventListener('change', () => {\n    // Tiny delay to let the theme update the hidden ID first\n    setTimeout(updateSKU, 50);\n  });\n\n  // Listen for the standard Shopify variant change event as a backup\n  document.addEventListener('variant:change', (event) => {\n    updateSKU();\n  });\n\n  // Run once on load\n  updateSKU();\n})();\n</script>"
